        <item>y*sin(&#952;/2)</item>
        <item>z*sin(&#952;/2)</item>
        <item>cos(&#952;/2)</item>
    </string-array>
    <string name="description_geomagnetic_rotation_vector">
		Similar to rotation-vector, but using a magnetometer instead of using a gyroscope.
	</string>

