    <string-array name="fields_game_rotation_vector">
        <item>elapsed-time-system</item>
        <item>elapsed-time-sensor</item>
        <item>x*sin(&#952;/2)</item>
        <item>y*sin(&#952;/2)</item>
        <item>z*sin(&#952;/2)</item>
        <item>cos(&#952;/2)</item>
        <item>estimated-heading-accuracy</item>
    </string-array>
    <string name="description_game_rotation_vector">
		Estimated-heading-accuracy is in radians. Identical to rotation-vector except that it
		doesn\'t use the geomagnetic field. Therefore the Y axis doesn\'t point north, but instead
		to some other reference.
	</string>

    <string-array name="fields_geomagnetic_rotation_vector">
